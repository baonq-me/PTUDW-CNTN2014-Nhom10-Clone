<div class="btn-group-vertical sidebar col-md-3 col-lg-3 visible-md visible-lg" id="category">
	<a href="#" class="btn btn-info disabled btn-category">Danh mục sản phẩm</a>
	<% sidebar.categorys.forEach(function(category){ %>
		<a href="/category/<%= category.slug %>" class="btn btn-info btn-category"><span class="<%= category.icon %>"></span><%= category.name %></a>
	<% }); %>
</div>